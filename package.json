  "keywords": [
    "a2a",
    "agent",
    "protocol",
    "agent-to-agent",
    "llm",
    "ai"
  ],
  "author": "Antonio Ono",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nio-ono/tete-a-tete"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@noble/hashes": "^2.0.1",
    "@noble/secp256k1": "^3.0.0"
  }
}
